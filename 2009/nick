#include <iostream>
#include <string>
using namespace std;

int main() {
    string s;
    getline(cin, s);

    for (int i = 0; i < (int)s.size(); i++) {
        if (isalpha(s[i])) {
            if ((i + 1) % 2 == 1) { 
                s[i] = toupper(s[i]); 
            } else {
                s[i] = tolower(s[i]);
            }
        }
    }

    cout << s;
    return 0;
}
