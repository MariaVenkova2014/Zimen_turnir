#include <iostream>
using namespace std;

int main() {
	int K, W;
	cin >> K >> W;
	int A[3], B[3];
	for (int i = 0; i < 3; i++) {
		cin >> A[i] >> B[i];
	}
	int count = 0;
	for (int i = 0; i < 8; i++) {
		int totalWeight = 0;
		int totalCapacity = 0;
		for (int j = 0; j < 3; j++) {
			if (i & (1 << j)) {
				totalWeight += A[j];
				totalCapacity += B[j];
			}
		}
		if (totalWeight <= W && totalCapacity >= K) {
			count++;
		}
	}
	cout << count << endl;
	return 0;
}
