#include <iostream>
using namespace std;

int main() {
	int n;
	cin >> n;
	int* arr = new int[n];

	for (int i = 0; i < n; i++) {
		cin >> arr[i];
	}

	int maxCount = 1;
	int count = 1;

	for (int i = 1; i < n; i++) {
		if (arr[i] > arr[i - 1]) {
			count++;
		} else {
			if (count > maxCount) {
				maxCount = count;
			}
			count = 1;
		}
		if (count > maxCount) {
			maxCount = count;
		}
	}

	cout << maxCount << endl;
	delete[] arr;
	return 0;
}
