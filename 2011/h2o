#include <iostream>
using namespace std;

int main() {
    int n;
    cin >> n;
    string s;
    cin >> s;

    int total = 0;
    for (int i = 0; i < n; i++) {
        char c = s[i];
        int mass = 0;
        if (c == 'H') mass = 1;
        else if (c == 'C') mass = 12;
        else if (c == 'N') mass = 14;
        else if (c == 'O') mass = 16;

        int mult = 1;
        if (i + 1 < n && isdigit(s[i + 1])) {
            mult = s[i + 1] - '0';
            i++;
        }

        total += mass * mult;
    }

    cout << total << endl;
}
